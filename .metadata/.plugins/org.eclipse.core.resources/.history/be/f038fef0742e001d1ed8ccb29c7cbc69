package polymorphism;

public class LgTV {
	public void turnOn() {
		System.out.println("엘지티비가 켜졌습니다.");
	}
	public void turnOff() {
		System.out.println("엘지티비가 꺼졌습니다.");
	}
	public void soundUp() {
		System.out.println("엘지티비 소리를 올립니다.");
	}
	public void soundDown() {
		System.out.println("엘지티비 소리를 줄입니다.");
	}
}
